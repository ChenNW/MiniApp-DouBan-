.container {

display: flex;
flex-direction: column;
align-items: center;
padding: 0 40rpx;

}
.logo{
    width: 144rpx;
    height: 144rpx;
    margin: 80rpx 0;
}
.btn-login{
    
    height: 80rpx;
    line-height: 80rpx;
    width: 100%;
    text-align: center;
    font-size: 36rpx;
    border-radius: 10rpx;
    
}
.weChat{
    background-color: #47BB5A;
    color: #ffffff;
    margin-bottom: 30rpx;
}
.douban{
    background-color: #ffffff;
    color: #47BB5A;
    border: 2rpx solid #47BB5A;
}
.aggrment{
    display: flex;
    position: absolute;
    bottom: 50rpx;
    font-size: 30rpx;
}
.aggrment .open{
    margin-left: 10rpx;
    color: #47BB5A;
}
.aggrment .privacy{
    color: #47BB5A;
}